package main

import "fmt"

func main() {

	matriz := [2][3]int{{1, 2, 3}, {4, 5, 6}}

	var linha, coluna int
	fmt.Print("Informe um índice de linha: ")
	fmt.Scan(&linha)
	fmt.Print("Informe um índice de coluna: ")
	fmt.Scan(&coluna)

	fmt.Printf("O valor armazenado na posição (%d,%d) é %d\n", linha, coluna, matriz[linha][coluna])
}
